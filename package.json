{
  "name": "antd-enhanced",
  "version": "0.0.0",
  "description": "Useful react components and hooks based on Ant Design",
  "type": "module",
  "files": ["dist"],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/style.css",
  "homepage": "https://github.com/ianzone/antd-enhanced#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianzone/antd-enhanced.git"
  },
  "scripts": {
    "lint": "biome check --fix src --reporter summary",
    "build": "vite build",
    "dev": "storybook dev -p 6006",
    "dev:cc": "rm ./node_modules/.cache -r && nr dev",
    "build-storybook": "storybook build",
    "postinstall": "lefthook install",
    "commit": "git add -A && git-cz",
    "release": "semantic-release",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@rapideditor/country-coder": "^5.3.0",
    "@twemoji/api": "^15.1.0",
    "libphonenumber-js": "^1.11.15",
    "react-icons": "^5.3.0"
  },
  "peerDependencies": {
    "antd": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-onboarding": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/builder-vite": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-vite": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "chromatic": "^11.19.0",
    "git-cz": "^4.9.0",
    "happy-dom": "^15.11.6",
    "lefthook": "^1.8.4",
    "semantic-release": "^24.2.0",
    "storybook": "^8.4.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "bugs": {
    "url": "https://github.com/ianzone/antd-enhanced/issues"
  }
}
